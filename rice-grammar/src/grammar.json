{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "rice",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_block"
      }
    },
    "_block": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "element"
        },
        {
          "type": "SYMBOL",
          "name": "property"
        }
      ]
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "//"
        },
        {
          "type": "PATTERN",
          "value": "[^\\n]*"
        }
      ]
    },
    "docstring": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "///"
        },
        {
          "type": "PATTERN",
          "value": "[^\\n]*"
        }
      ]
    },
    "classname": {
      "type": "PATTERN",
      "value": "[A-Z][a-zA-Z0-9_]*"
    },
    "propname": {
      "type": "PATTERN",
      "value": "[a-z][a-zA-Z0-9_]*"
    },
    "element": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "classname"
            },
            {
              "type": "STRING",
              "value": "{"
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "_block"
              }
            },
            {
              "type": "STRING",
              "value": "}"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "classname"
        }
      ]
    },
    "property": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "propname"
            },
            {
              "type": "STRING",
              "value": ":"
            },
            {
              "type": "SYMBOL",
              "name": "_value"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "propname"
        }
      ]
    },
    "_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "boolean"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "pixels"
        },
        {
          "type": "SYMBOL",
          "name": "fraction"
        },
        {
          "type": "SYMBOL",
          "name": "percentage"
        }
      ]
    },
    "boolean": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "true"
        },
        {
          "type": "STRING",
          "value": "false"
        }
      ]
    },
    "string": {
      "type": "PATTERN",
      "value": "\"(?:[^\"\\\\]|\\\\.)*\""
    },
    "pixels": {
      "type": "PATTERN",
      "value": "[0-9]+px"
    },
    "fraction": {
      "type": "PATTERN",
      "value": "[0-9]+(?:\\.[0-9]+)?fr"
    },
    "percentage": {
      "type": "PATTERN",
      "value": "[0-9]+(?:\\.[0-9]+)?%"
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "SYMBOL",
      "name": "docstring"
    },
    {
      "type": "PATTERN",
      "value": "\\s+"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}